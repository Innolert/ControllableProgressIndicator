<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item >
        <inset xmlns:android="http://schemas.android.com/apk/res/android"
            android:insetBottom="4.333333333dip"
            android:insetLeft="4.333333333dip" android:insetRight="4.333333333dip" android:insetTop="4.333333333dip"
            android:visible="true" >
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:dither="true"
            android:shape="oval"

            android:useLevel="true">

            <size
                android:width="36dp"
                android:height="36dp" />
            <solid android:color="#089dd8" />

        </shape>
        </inset>
    </item>
    <item >
        <inset xmlns:android="http://schemas.android.com/apk/res/android"
            android:insetBottom="3dip"
            android:insetLeft="3dip" android:insetRight="3dip" android:insetTop="3dip"
            android:visible="true" >
            <shape xmlns:android="http://schemas.android.com/apk/res/android"
                android:dither="true"
                android:shape="oval"

                android:useLevel="true">

                <size
                    android:width="32dp"
                    android:height="32dp" />
                <solid android:color="#4ab8e4" />

            </shape>
        </inset>
    </item>
    <item >
        <inset xmlns:android="http://schemas.android.com/apk/res/android"
            android:insetBottom="2dip"
            android:insetLeft="2dip" android:insetRight="2dip" android:insetTop="2dip"
            android:visible="true" >
            <shape xmlns:android="http://schemas.android.com/apk/res/android"
                android:dither="true"
                android:shape="oval"

                android:useLevel="true">

                <size
                    android:width="32dp"
                    android:height="32dp" />
                <solid android:color="#089dd8" />

            </shape>
        </inset>
    </item>

    <item android:left="3dp" android:right="3dp" android:bottom="2dp" android:top="4dp" android:drawable="@drawable/checkmark"/>

</layer-list>