<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>

        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:dither="true"
            android:shape="oval"
            android:useLevel="true">

            <corners android:radius="0dp" />

            <size
                android:width="44.333333333dp"
                android:height="44.333333333dp" />
        </shape>
    </item>
    <item>
        <inset xmlns:android="http://schemas.android.com/apk/res/android"
            android:insetBottom="3dip"
            android:insetLeft="3dip"
            android:insetRight="3dip"
            android:insetTop="3dip">
            <shape xmlns:android="http://schemas.android.com/apk/res/android"
                android:dither="true"
                android:shape="oval"
                android:useLevel="true">

                <size
                    android:width="38dp"
                    android:height="38dp" />
                <solid android:color="#089dd8" />

            </shape>
        </inset>
    </item>
    <item>
        <inset xmlns:android="http://schemas.android.com/apk/res/android"
            android:insetBottom="3dip"
            android:insetLeft="3dip"
            android:insetRight="3dip"
            android:insetTop="3dip">
            <shape xmlns:android="http://schemas.android.com/apk/res/android"
                android:dither="true"
                android:shape="oval"

                android:useLevel="true">

                <size
                    android:width="32dp"
                    android:height="32dp" />
                <solid android:color="#4ab8e4" />

            </shape>
        </inset>
    </item>
    <item>
        <inset xmlns:android="http://schemas.android.com/apk/res/android"
            android:insetBottom="3dip"
            android:insetLeft="3dip"
            android:insetRight="3dip"
            android:insetTop="3dip">
            <shape xmlns:android="http://schemas.android.com/apk/res/android"
                android:dither="true"
                android:shape="oval"

                android:useLevel="true">

                <size
                    android:width="26dp"
                    android:height="26dp" />
                <solid android:color="#089dd8" />

            </shape>
        </inset>
    </item>
    <item android:drawable="@drawable/ic_check_white_24dp" />


</layer-list>